		"eslint-plugin-svelte": "^2.46.0",
		"prettier": "^3.4.0",
		"prettier-plugin-svelte": "^3.3.0",
		"svelte": "^5.16.0",
		"svelte-check": "^4.1.0",
		"typescript": "^5.0.0",
